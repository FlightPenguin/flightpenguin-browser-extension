{
  "name": "FlightPenguin",
  "version": "1.0.2",
  "description": "Search flights",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.southwest.com/air/booking/select.html?*"],
      "css": ["shared/contentScript.css"],
      "js": [
        "shared/sentry.js",
        "shared/helpers.js",
        "southwest/contentScript.js"
      ]
    },
    {
      "matches": ["https://www.southwest.com/air/booking/index.html?*"],
      "js": [
        "shared/sentry.js",
        "southwest/emptySearchContentScript.js"
      ]
    },
    {
      "matches": ["https://www.skyscanner.com/transport/flights/*"],
      "css": ["shared/contentScript.css"],
      "js": [
        "shared/nameMaps/airlineMapGlobalScope.js",
        "shared/sentry.js",
        "shared/helpers.js",
        "skyscanner/contentScript.js"
      ]
    },
    {
      "matches": ["https://www.skyscanner.com/transport/flights-from/*"],
      "css": ["shared/contentScript.css"],
      "js": [
        "shared/sentry.js",
        "shared/helpers.js",
        "skyscanner/emptySearchContentScript.js"
      ]
    },
    {
      "matches": ["https://www.expedia.com/Flights-Search?*"],
      "css": ["shared/contentScript.css"],
      "js": [
        "shared/nameMaps/airlineMapGlobalScope.js",
        "shared/sentry.js",
        "shared/helpers.js",
        "expedia/contentScript.js"
      ]
    }
  ],
  "permissions": ["identity", "https://www.googleapis.com/"],
  "key": "",
  "oauth2": {
    "client_id": "82466302556-jujsfqptcild0kjidp1tspr9pghdva92.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/plus.login", "email"]
  },
  "browser_action": {
    "default_title": "FlightPenguin"
  },
  "icons": {
    "16": "icons/icon.png",
    "48": "icons/icon.png",
   "128": "icons/icon128.png"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://browser.sentry-cdn.com https://www.gstatic.com; object-src 'self'",
  "web_accessible_resources": ["index.html"]
}
