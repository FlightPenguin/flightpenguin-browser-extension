{
  "name": "Flight Penguin Browser Extension",
  "version": "1.0",
  "description": "Best one in town!",
  "background": {
    "page": "background.html",
    "persistent": false
  },
  "content_scripts": [
    {
      "matches": ["https://www.priceline.com/m/fly/search/*"],
      "js": ["priceline/contentScript.js"]
    },
    {
      "matches": ["https://www.southwest.com/air/booking/select.html?*"],
      "css": ["shared/contentScript.css"],
      "js": ["shared/sentry.js", "southwest/contentScript.js"]
    },
    {
      "matches": ["https://www.southwest.com/air/booking/index.html?*"],
      "js": ["shared/sentry.js", "southwest/emptySearchContentScript.js"]
    },
    {
      "matches": ["https://www.skyscanner.com/transport/flights/*"],
      "css": ["shared/contentScript.css"],
      "js": ["shared/sentry.js", "skyscanner/contentScript.js"]
    },
    {
      "matches": ["https://www.expedia.com/Flights-Search?*"],
      "css": ["shared/contentScript.css"],
      "js": ["shared/sentry.js", "expedia/contentScript.js"]
    }
  ],
  "permissions": ["identity", "https://www.googleapis.com/"],
  "key": "",
  "oauth2": {
    "client_id": "82466302556-eom46d7cin23ce6ndfg28gclfmst4qth.apps.googleusercontent.com",
    "scopes": ["https://www.googleapis.com/auth/plus.login", "email"]
  },
  "browser_action": {
    "default_title": "Flight Penguin"
  },
  "manifest_version": 2,
  "content_security_policy": "script-src 'self' https://www.google-analytics.com https://browser.sentry-cdn.com https://www.gstatic.com; object-src 'self'"
}
